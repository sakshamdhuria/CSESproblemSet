#include <bits/stdc++.h>
using namespace std;
 
#define int long long 
const long long  mod =1e9+7;
int32_t main() {
    ios::sync_with_stdio(false);
    cin.tie(NULL);
    
    int n;
    cin>>n;
    int sum = n*(n+1)/2;
    for(int i=1; i<n; i++){
        int a;
        cin>>a;
        sum-=a;
    }
    cout<<sum;
 
 
    return 0;
}
