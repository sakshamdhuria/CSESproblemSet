#include <bits/stdc++.h>
using namespace std;
 
#define int long long 
const long long  mod =1e9+7;
int32_t main() {
    ios::sync_with_stdio(false);
    cin.tie(NULL);
    int n;
    cin>>n;
    while(n!=1){
        cout<<n<<" ";
        if(n%2==0){
            n/=2;
        }else{
            n=3*n+1;
        }
    }
    cout<<1;
    return 0;
}
