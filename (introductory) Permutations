#include <bits/stdc++.h>
using namespace std;

#define int long long 
const long long  mod =1e9+7;
int32_t main() {
    ios::sync_with_stdio(false);
    cin.tie(NULL);
    
    int n; 
    cin>>n;
    if(n==1){
        cout<<"1";
        return 0;
    }
    if(n<=3){
        cout<<"NO SOLUTION";
        return 0;
    }else if(n==4){
        cout<<"3 1 4 2";
        return 0;
    }
    for(int i=1; i<=n; i++){
        if(i%2==0){
            cout<< i<< " ";
        }
    }
    for(int i=1; i<=n; i++){
        if(i%2!=0){
            cout<< i<< " ";
        }
    }
    return 0;
}
