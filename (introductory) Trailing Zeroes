#include <bits/stdc++.h>
using namespace std;

#define int long long 
const long long  mod =1e9+7;
int32_t main() {
    ios::sync_with_stdio(false);
    cin.tie(NULL);
    int n;
    cin>>n;
    int f =(int) (log(n)/log(5));
    int ans=0;
    for(int i=1; i<=f; i++){
        ans+=(n)/((int) (pow(5, i)));
    }
    cout<<ans;
    return 0;
}
